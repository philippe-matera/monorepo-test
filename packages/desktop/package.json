{
  "name": "@matera-tech/desktop",
  "version": "6.0.0-alpha.rrd.3",
  "sideEffects": false,
  "private": false,
  "description": "Matera - Shared desktop components",
  "babel": {
    "presets": [
      "@babel/preset-react",
      "@babel/preset-env"
    ],
    "plugins": [
      [
        "module-resolver",
        {
          "alias": {
            "shared": "./src/shared",
            "src": "./src",
            "ui": "./src/ui",
            "utils": "./src/utils",
            "hooks": "./src/hooks",
            "lib": "./src/lib",
            "resource": "./src/resource"
          }
        }
      ]
    ]
  },
  "main": "dist/index.js",
  "files": [
    "dist/**/*"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@matera-tech"
  },
  "scripts": {
    "build": "yarn install --force && rm -rf dist && mkdir dist &&  babel ./src -d dist --copy-files",
    "dev": "babel ./src -d dist --copy-files --watch",
    "pre-dev": "babel ./src -d dist --copy-files && yalc publish",
    "lint": "./node_modules/eslint/bin/eslint.js src/ --ext .js,.jsx --fix",
    "stylelint": "stylelint '**/*.jsx' --fix",
    "precommit": "lint-staged",
    "storybook": "start-storybook -s ./static -p 6006 --no-dll",
    "pre-release": "npx lerna run --concurrency 1 --stream build && npx lerna publish",
    "build-storybook": "build-storybook --no-dll",
    "lerna-build-push": "cd .. && git add . && git commit --allow-empty -m \"lerna build & release\" && git push -f",
    "update-master": "git checkout desktop && git pull && git checkout utils && git pull && git checkout master && git pull && git merge desktop && git push && git merge utils && git push",
    "release": "yarn update-master && npx lerna run --concurrency 1 --stream build && yarn lerna-build-push && npx lerna publish && yarn update-branches",
    "update-branches": "git checkout desktop && git merge master && git push && git checkout utils && git merge master && git push",
    "prettify": "./node_modules/.bin/prettier --config .prettierrc.json --loglevel silent \"src/**/*.{js,jsx,css,md}\" --write"
  },
  "lint-staged": {
    "./src/**/*.{js,jsx}": [
      "stylelint --fix"
    ],
    "*.{js,jsx}": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/matera-tech/npm-packages.git"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@matera-tech/utils": "^3.0.10",
    "@mdi/react": "1.5.0",
    "@popperjs/core": "^2.9.3",
    "@tammo/react-iban": "^0.0.2",
    "emoji-mart": "^3.0.1",
    "i18next": "^20.0.0",
    "iso-3166-1-alpha-2": "^1.0.0",
    "json-loader": "^0.5.7",
    "libphonenumber-js": "1.9.24",
    "lodash.debounce": "^4.0.8",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.26",
    "nanoid": "^3.1.30",
    "numeral": "^2.0.6",
    "prop-types": "^15.7.2",
    "react": "18.0.0",
    "react-datepicker": "^4.7.0",
    "react-dropzone": "10.2.2",
    "react-i18next": "^11.3.1",
    "react-number-format": "4.9.1",
    "react-phone-number-input": "3.1.27",
    "react-popper": "^2.2.5",
    "react-router-dom": "6.2.2",
    "react-select": "3.1.0",
    "react-spinners": "0.11.0",
    "react-tippy": "^1.4.0",
    "react-toastify": "^8.2.0",
    "styled-components": "5.3.3",
    "underscore": "^1.10.2",
    "velocity-react": "1.4.3",
    "yaml-loader": "^0.6.0"
  },
  "peerDependencies": {
    "formik": "^2.1.4",
    "react-async-script": "^1.0.0",
    "react-fast-compare": "^3.0.0",
    "react-places-autocomplete": "https://github.com/Romusc/react-places-autocomplete.git",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/cli": "7.16.0",
    "@babel/core": "7.16.0",
    "@babel/preset-env": "7.16.0",
    "@babel/preset-react": "7.16.0",
    "@matera-tech/eslint-config": "0.1.2",
    "@mdi/react": "1.5.0",
    "@storybook/addon-actions": "6.3.12",
    "@storybook/addon-essentials": "6.3.12",
    "@storybook/addon-links": "6.3.12",
    "@storybook/react": "6.3.12",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.2.3",
    "babel-plugin-module-resolver": "4.1.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-babel-module": "5.3.1",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.26.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "filesize": "8.0.6",
    "formik": "2.2.9",
    "husky": "7.0.4",
    "lint-staged": "11.2.6",
    "prettier": "2.4.1",
    "react-async-script": "1.2.0",
    "react-dom": "18.0.0",
    "react-fast-compare": "3.2.0",
    "react-is": "17.0.2",
    "react-places-autocomplete": "https://github.com/Romusc/react-places-autocomplete.git",
    "react-query": "3.27.0",
    "stylelint": "13.13.1",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-standard": "22.0.0",
    "stylelint-order": "4.1.0",
    "stylelint-prettier": "1.2.0"
  },
  "author": "Matera",
  "license": "UNLICENSED",
  "gitHead": "c645798a0920ddb5fcab18453024df125d7fd3fd"
}
